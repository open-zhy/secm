  "version": "0.2.0",
  "configurations": [
    {
      "name": "Create secret",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceRoot}/main.go",
      "args": [
        "create",
        "fixtures/secret-sample.txt",
        "--name",
        "sample secret",
        "--tags",
        "test",
        "-f",
        "text",
        // "--profile",
        // "ed"
      ]
    },
    {
      "name": "Id",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceRoot}/main.go",
      "args": [
        "id",
        "--profile",
        "prime521v1",
        "--plugins-dir",
        "${cwd}/build/plugins",
      ]
    },
    {
      "name": "Keygen",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceRoot}/main.go",
      "args": [
        "generate",
        "-t",
        "p256",
        "--profile",
        "ed",
      ]
    }
  ]
}